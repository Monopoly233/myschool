{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "#000000"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#0073AA"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#005177"
				},
				{
					"name": "Accent",
					"slug": "accent",
					"color": "#00A0D2"
				},
				{
					"name": "Background",
					"slug": "background",
					"color": "#FFFFFF"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "1rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "2rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "3rem"
				},
				{
					"name": "Extra Large",
					"slug": "extra-large",
					"size": "4rem"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"writingMode": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"max": "1rem",
						"min": "0.875rem"
					},
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.5rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			],
			"fontFamilies": [
				{
					"name": "System Font",
					"slug": "system-font",
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--base)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"top": "var(--wp--preset--spacing--medium)",
				"right": "var(--wp--preset--spacing--medium)",
				"bottom": "var(--wp--preset--spacing--medium)",
				"left": "var(--wp--preset--spacing--medium)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.6"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.2"
				},
				"color": {
					"text": "var:preset|color|primary"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "1.6"
				},
				"color": {
					"text": "var:preset|color|primary"
				}
			},
			"core/button": {
				"color": {
					"background": "var:preset|color|secondary",
					"text": "var:preset|color|base"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|accent",
						"text": "var:preset|color|base"
					}
				},
				"spacing": {
					"padding": {
						"top": "1rem",
						"right": "2rem",
						"bottom": "1rem",
						"left": "2rem"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "700"
				}
			},
			"core/quote": {
				"color": {
					"background": "var:preset|color|background",
					"text": "var:preset|color|primary"
				},
				"border": {
					"color": "var:preset|color|secondary",
					"width": "4px",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|medium",
						"right": "var:preset|spacing|medium",
						"bottom": "var:preset|spacing|medium",
						"left": "var:preset|spacing|medium"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"fontStyle": "italic"
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--accent)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				}
			}
		}
	},
	"templates": {
		"index": {
			"title": "Index",
			"postTypes": ["post", "page"]
		},
		"page-staff-archive": {
			"title": "Staff Archive",
			"postTypes": ["page"],
			"file": "templates/page-staff-archive.html"
		},
		"page-staff": {
			"title": "Staff Page",
			"postTypes": ["page"],
			"file": "templates/page-staff.html"
		},
		"archive-staff": {
			"title": "Staff Archive",
			"postTypes": ["staff"],
			"file": "templates/archive-staff.html"
		},
		"archive-student": {
			"title": "Student Archive",
			"postTypes": ["student"],
			"file": "templates/archive-student.html"
		},
		"single-student": {
			"title": "Single Student",
			"postTypes": ["student"],
			"file": "templates/single-student.html"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header",
			"syncStatus": "synced"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer",
			"syncStatus": "synced"
		}
	],
	"customTemplates": [
		{
			"name": "index",
			"title": "Index",
			"postTypes": ["post", "page"],
			"file": "templates/index.html"
		},
		{
			"name": "staff-archive",
			"title": "Staff Archive",
			"postTypes": ["page"],
			"file": "templates/page-staff-archive.html"
		},
		{
			"name": "staff",
			"title": "Staff",
			"postTypes": ["staff"],
			"file": "templates/page-staff.html"
		},
		{
			"name": "student-archive",
			"title": "Student Archive",
			"postTypes": ["page"],
			"file": "templates/archive-student.html"
		},
		{
			"name": "student",
			"title": "Student",
			"postTypes": ["student"],
			"file": "templates/single-student.html"
		},
		{
			"name": "search",
			"title": "Search",
			"postTypes": ["post", "page"],
			"file": "templates/search.html"
		}
	]
}
